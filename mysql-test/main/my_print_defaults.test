--echo #
--echo # Beginning of 10.5 Test
--echo #
--echo #
--echo # MDEV-24248: my_print_defaults is not taking all the values when using
--echo # -e option which is called from mysql.server (extra_args).
--echo #

--echo # checking for mysql.server

--write_file $MYSQLTEST_VARDIR/tmp/tmp1.cnf

[mariadb-10.5]
key_buffer_size=20M
max_allowed_packet=250M
table_open_cache=1000
table_definition_cache=2000
read_buffer_size=1M
thread_cache_size=8
max_connections=1024
long_query_time=60
slow_query_log=1
EOF

--exec $MYSQL_MY_PRINT_DEFAULTS -e $MYSQLTEST_VARDIR/tmp/tmp1.cnf --mysqld mysql.server
--exec $MYSQL_MY_PRINT_DEFAULTS --defaults-extra-file=$MYSQLTEST_VARDIR/tmp/tmp1.cnf --mysqld mysql.server

--remove_file $MYSQLTEST_VARDIR/tmp/tmp1.cnf

--echo  checking for mysqltest1

--write_file $MYSQLTEST_VARDIR/tmp/tmp2.cnf
[mysqltest1]
port=1234
EOF

--exec $MYSQL_MY_PRINT_DEFAULTS -e $MYSQLTEST_VARDIR/tmp/tmp2.cnf mysqltest1
--exec $MYSQL_MY_PRINT_DEFAULTS -c $MYSQLTEST_VARDIR/tmp/tmp2.cnf mysqltest1

--exec $MYSQL_MY_PRINT_DEFAULTS --defaults-extra-file=$MYSQLTEST_VARDIR/tmp/tmp2.cnf mysqltest1
--exec $MYSQL_MY_PRINT_DEFAULTS --defaults-file=$MYSQLTEST_VARDIR/tmp/tmp2.cnf mysqltest1

--remove_file $MYSQLTEST_VARDIR/tmp/tmp2.cnf

--echo # End of 10.5 test
